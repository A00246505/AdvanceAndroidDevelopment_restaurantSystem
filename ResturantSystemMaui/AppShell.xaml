<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ResturantSystemMaui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:ResturantSystemMaui.Views"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate views:SignInPage}"
        Route="SignInPage" />


    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate views:NewPage1}"
        Route="SignUpPage" />

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate views:MenuItem}"
        Route="MenuItemPage" />

</Shell>
